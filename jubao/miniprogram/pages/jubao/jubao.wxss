page{
  background: #f7f8fa;
}

.container{
  padding:10px 20px;
}
form{
  width: 100%;
  /* background-color: #fff; */
}

.borde{
  height: 100%;
}

.left{
  float: left;
  font-size: 14px;
  line-height: 40px;
  width: 50px;
  height: 50px;
}
.right{
  float: left;
}
.inp{
  height: 40px;
  width: 80%;
  padding-left: 3px;
  line-height: 40px;
  border: 1px solid #666;
  border-radius: 5px;
  margin-bottom: 15px;
}
.desc{
  height: 200px;
  width: 80%;
  padding-left: 3px;
  line-height: 40px;
  border: 1px solid #666;
  border-radius: 5px;
  margin-bottom: 15px;
}
.cameraBtn{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80px;
  height: 80px;
  border:1px solid #999;
  border-radius: 5px;
  margin-bottom: 15px;
  margin-top: 10px;
}
.subBtn{
  margin-top: 15px;
  margin-left: 80px;
  clear: both;
}
.img{
  width: 80px;
  height: 80px;
  margin: 5px;
  margin-left: 0px;
}

.img1{
  width: 80px;
  height: 80px;
  margin: 5px;
  margin-left: 0px;
  position: relative;
}

/* .img1::after{ */
.del{
  position: absolute;
  top:5px;
  right:5px;
  color: #FFB800;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  text-align: center;
  line-height: 20px;
  font-size: 16px;
  /* content:'x'; */
}

.upBtn{
  width: 100%;
  background-color: #1989fa;
  line-height: 20px;
  border-radius: 5px;
  text-align: center;
  color:#fff;
  margin-top: 15px;
}


/* page{
  overflow: hidden;
} */

.alr{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.camera{
  width: 100%;
  height: 800px;
  position: relative;
}
.btn{
  position: absolute;
  bottom:20px;
  left: 40%;
  width: 50px;
  height: 50px;
  background-color: #fff;
  border:10px solid #999;
  border-radius: 50%;
}